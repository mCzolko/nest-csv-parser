{
  "name": "nest-csv-parser",
  "version": "2.0.0",
  "description": "CSV parser for NestJS framework",
  "author": "Michael Czolko <michael@czolko.cz>",
  "license": "MIT",
  "scripts": {
    "compile": "tsc -p tsconfig.json",
    "build": "rm tsconfig.tsbuildinfo || rm -rf dist && tsc -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch --verbose",
    "test:cov": "jest --coverage",
    "test:cov-ci": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next"
  },
  "dependencies": {
    "@nestjs/common": "7.2.0",
    "@nestjs/core": "7.2.0",
    "class-transformer": "0.2.3",
    "csv-parser": "2.3.3",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.5"
  },
  "devDependencies": {
    "@nestjs/testing": "7.2.0",
    "@types/jest": "26.0.4",
    "@types/node": "11.13.20",
    "coveralls": "3.1.0",
    "jest": "24.9.0",
    "prettier": "2.0.5",
    "ts-jest": "26.1.1",
    "ts-node": "8.10.2",
    "tsc-watch": "3.0.1",
    "tsconfig-paths": "3.9.0",
    "tslint": "5.20.0",
    "typescript": "3.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "lib",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
