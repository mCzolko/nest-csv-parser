{
  "name": "nest-csv-parser",
  "version": "2.0.0",
  "description": "CSV parser for NestJS framework",
  "author": "Michael Czolko <michael@czolko.cz>",
  "license": "MIT",
  "scripts": {
    "compile": "tsc -p tsconfig.json",
    "build": "rm tsconfig.tsbuildinfo || rm -rf dist && tsc -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch --verbose",
    "test:cov": "jest --coverage",
    "test:cov-ci": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next"
  },
  "dependencies": {
    "@nestjs/common": "6.6.7",
    "@nestjs/core": "6.6.7",
    "class-transformer": "0.2.3",
    "csv-parser": "2.3.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.3"
  },
  "devDependencies": {
    "@nestjs/testing": "6.6.7",
    "@types/jest": "24.0.18",
    "@types/node": "11.13.20",
    "coveralls": "3.0.6",
    "jest": "24.9.0",
    "prettier": "1.18.2",
    "ts-jest": "25.5.1",
    "ts-node": "8.3.0",
    "tsc-watch": "3.0.1",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.20.0",
    "typescript": "3.5.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "lib",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
